LIMIT=$1
echo "Time limit: $LIMIT"

run() {
  PREFIX=$1
  OPT=$2
  LOWER=$3
  UPPER=$4
  echo $PREFIX $OPT
  for i in $(seq $LOWER $UPPER); do
    echo -n "$i ";
    hbmclimit $LIMIT Mergesort.hs $OPT o$PREFIX$i > /dev/null || break;
  done;
}

run minj normal  0 29
run minj mmerge  0 29
run minj unlabel 0 29

run mnub normal  0 29
run mnub mmerge  0 29
run mnub unlabel 0 29

run iinj normal  0 29
run inub normal  0 29

run qinj normal  0 29
run qnub normal  0 29

