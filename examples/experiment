run() {
  PREFIX=$1
  OPT=$2
  UPPER=$3
  echo $PREFIX $OPT
  for i in $(seq 0 $UPPER); do
    echo -n "$i ";
    hbmclimit 60 Mergesort.hs $OPT o$PREFIX$i > /dev/null;
  done;
}

run iinj normal  30
run inub normal  10

run minj normal  25
run minj mmerge  25
run minj unlabel 20

run mnub normal  10
run mnub mmerge  10
run mnub unlabel 10
